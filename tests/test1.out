- SOURCE_FILE
  - RULE
    - "rule" RULE_KW
    - " " WHITESPACE
    - "test" IDENTIFIER
    - "\n" WHITESPACE
    - BLOCK_EXPR
      - "{" L_BRACE
      - "\n\t" WHITESPACE
      - STRINGS
        - "strings" STRINGS_KW
        - ":" COLON
        - "\n\t\t" WHITESPACE
        - VARIABLE_STMT
          - "$a" VARIABLE
          - " " WHITESPACE
          - "=" ASSIGN
          - " " WHITESPACE
          - PATTERN
            - "\"foo\"" STRING_LIT
      - "\n\t" WHITESPACE
      - CONDITION
        - "condition" CONDITION_KW
        - ":" COLON
        - "\n\t\t" WHITESPACE
        - EXPRESSION_STMT
          - LITERAL
            - "$a" VARIABLE
      - "\n" WHITESPACE
      - "}" R_BRACE
  - "\n" WHITESPACE
