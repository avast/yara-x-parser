- SOURCE_FILE
  - "//Global comment" COMMENT
  - "\n\n" WHITESPACE
  - RULE
    - "//Rule comment" COMMENT
    - "\n" WHITESPACE
    - "rule" RULE_KW
    - " " WHITESPACE
    - "test" IDENTIFIER
    - "\n" WHITESPACE
    - BLOCK_EXPR
      - "{" L_BRACE
      - "\n\t" WHITESPACE
      - "//Rule block comment" COMMENT
      - "\n\n\t" WHITESPACE
      - "//String comment" COMMENT
      - "\n\t" WHITESPACE
      - ERROR
        - "string" IDENTIFIER
      - ":" COLON
      - "\n\t\t" WHITESPACE
      - VARIABLE_STMT
        - "$b" VARIABLE
        - " " WHITESPACE
        - "=" ASSIGN
        - " " WHITESPACE
        - PATTERN
          - "\"bar\"" STRING_LIT
      - "\n\t" WHITESPACE
      - CONDITION
        - "condition" CONDITION_KW
        - ":" COLON
        - "\n\t\t" WHITESPACE
        - EXPRESSION_STMT
          - EXPRESSION
            - EXPRESSION
              - LITERAL
                - "$b" VARIABLE
              - " " WHITESPACE
              - "and" AND_KW
              - " " WHITESPACE
              - PREFIX_EXPR
                - "not" NOT_KW
                - " " WHITESPACE
                - LITERAL
                  - "true" TRUE_KW
            - " " WHITESPACE
            - "or" OR_KW
            - " " WHITESPACE
            - LITERAL
              - "false" FALSE_KW
      - "\n" WHITESPACE
      - "}" R_BRACE
  - "\n" WHITESPACE
